{
    "@context": "http://api.pax-db.org/orthology-context.jsonld",
    "@id": "/protein/<%= protein %>",
    "protein": {
        "@id" : "http://api.pax-db.org/protein/<%= protein %>",
        "id": "<%= protein %>"
    },
    "@type": "OrthologGroups",
    "members": [
    <% for (var numLevels = taxonomicLevels.length, i = 0; i < numLevels; i++) {
        var level = taxonomicLevels[i];    %>
        {

        "@id": "/protein/<%= protein %>/ortholog_groups/<%= level %>",
        "@type": "OrthologGroup",
        "taxonomicLevel": "<%= level %>"
        }
        <% if (i < numLevels - 1 ) { %> , <% } %>
        <% } %>
    ]
}
